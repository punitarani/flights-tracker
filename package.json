{
  "name": "flights-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev & next dev",
    "build": "wrangler deploy --dry-run && next build",
    "start": "wrangler dev & next start",
    "lint": "biome check --write --unsafe",
    "format": "biome format --write",
    "test": "BUN_TEST_CONCURRENCY=1 bun test --timeout 60000 --preload ./src/test/setup.ts src",
    "test:watch": "BUN_TEST_CONCURRENCY=1 bun test --watch --timeout 60000 --preload ./src/test/setup.ts src",
    "test:fli": "RUN_INTEGRATION_TESTS=true BUN_TEST_CONCURRENCY=1 bun test --timeout 100000 --preload ./src/test/setup.ts src/lib/fli",
    "test:workers": "BUN_TEST_CONCURRENCY=1 bun test --timeout 60000 src/workers/**/*.test.ts",
    "test:workers:watch": "BUN_TEST_CONCURRENCY=1 bun test --watch --timeout 60000 src/workers/**/*.test.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "worker:tail": "wrangler tail",
    "trigger:alerts": "bun scripts/trigger-alerts.ts"
